<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SelectToMakeAppController">
   <children>
      <TableView fx:id="allCustTable" layoutX="44.0" layoutY="108.0" prefHeight="205.0" prefWidth="532.0">
         <columns>
            <TableColumn fx:id="colCustomerId" prefWidth="50.0" text="ID" />
            <TableColumn fx:id="colCustomerName" prefWidth="284.0" text="Name" />
            <TableColumn fx:id="colCustomerLastUpdate" prefWidth="121.0" text="Last Update" />
         </columns>
      </TableView>
      <Button fx:id="returnToMain" layoutX="454.0" layoutY="339.0" mnemonicParsing="false" onAction="#returnToMain" text="Return To Main Menu">
         <font>
            <Font size="11.0" />
         </font></Button>
      <Button fx:id="makeAppButton" layoutX="27.0" layoutY="339.0" mnemonicParsing="false" onAction="#moveToCreateAppointment" text="Make New Customer Appointment">
         <font>
            <Font size="11.0" />
         </font></Button>
      <Text layoutX="211.0" layoutY="66.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Create Appointment">
         <font>
            <Font size="22.0" />
         </font>
      </Text>
      <Text layoutX="190.0" layoutY="92.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select a Customer to Create an Appointment" />
      <Button fx:id="editRemoveButton" onAction="#moveToEditRemoveAppointment"  layoutX="237.0" layoutY="340.0" mnemonicParsing="false" text="Edit/Remove Customer Appointment">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
